<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.project.TertiaryController">
    <children>
        <Label text="CONTATOS">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <AnchorPane fx:id="anchorPane" prefHeight="251.0" prefWidth="394.0">
         <children>
            <VBox fx:id="idVbox">
               <children>
                  <Pane style="-fx-background-color: #f5fffb; -fx-border-radius: 10; -fx-border-color: #c0c0c0; -fx-background-radius: 10;">
                     <children>
                        <Pane layoutX="15.0" layoutY="14.0" style="-fx-background-color: #f5fffb;">
                           <children>
                              <Label fx:id="labelNome" style="-fx-border-width: 0;" text="Nome:" />
                           </children>
                           <padding>
                              <Insets right="4.0" />
                           </padding>
                        </Pane>
                        <Pane layoutX="14.0" layoutY="31.0" style="-fx-background-color: #f5fffb;">
                           <children>
                              <Label fx:id="labelSobrenome" prefHeight="17.0" text="Sobrenome:" />
                           </children>
                           <padding>
                              <Insets right="4.0" />
                           </padding>
                        </Pane>
                        <Pane layoutX="15.0" layoutY="48.0" style="-fx-background-color: #f5fffb;">
                           <children>
                              <Label fx:id="labelTelefone" text="Telefone:" />
                           </children>
                           <padding>
                              <Insets right="4.0" />
                           </padding>
                        </Pane>
                        <Pane layoutX="14.0" layoutY="65.0" style="-fx-background-color: #f5fffb;">
                           <children>
                              <Label fx:id="labelEmail" text="Email:" />
                           </children>
                           <padding>
                              <Insets right="4.0" />
                           </padding>
                        </Pane>
                     </children>
                     <padding>
                        <Insets bottom="6.0" />
                     </padding>
                  </Pane>
               </children>
            </VBox>
         </children>
      </AnchorPane>
        <Button fx:id="tertiaryButton" onAction="#switchToPrimary" text="Ir para tela inicial" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
